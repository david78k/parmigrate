#prefix=rand-10-r1
prefix=rand-6-r2
dstat=${prefix}.dstat
bw=${prefix}.bw

# for controller
#grep controller $prefix | tail -n +2 | awk '{print $3,$4,$5,$6,$7}'> ${prefix}.controller

# for static migrations
sed 's/B//g;s/M/000000/g;s/k/000/g' $dstat | tail -n +2 > $bw

octave genplot.m $prefix

git add .
